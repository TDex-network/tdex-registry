{
  "$id": "https://raw.githubusercontent.com/TDex-network/tdex-registry/master/schema.json",
  "description": "A curated list of TDex Liquidity Providers",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "The tdex liquidity provider name",
        "minLength": 1,
        "maxLength": 40
      },
      "endpoint": {
        "type": "string",
        "description": "The provider endpoint",
        "format": "uri"
      }
    }
  }
}